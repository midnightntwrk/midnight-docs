{
    "cleanUrls": true,
    "rewrites": [{ "source": "/(.*)", "destination": "/" }],
    "redirects": [
      {
        "source": "/academy",
        "destination": "https://docs.midnight.network",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "no-cache" },
          { "key": "Pragma", "value": "no-cache" }
        ]
      }
    ]
  }